<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!-- ENTITY CONVERTER -->
	<converter>
		<converter-id>entityConverter</converter-id>
		<converter-class>util.EntityConverter</converter-class>
	</converter>
	<!-- *** MANAGED BEANS *** -->
	<managed-bean>
		<managed-bean-name>categoriaMBean</managed-bean-name>
		<managed-bean-class>managedBeans.CategoriaMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editoraMBean</managed-bean-name>
		<managed-bean-class>managedBeans.EditoraMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>livroMBean</managed-bean-name>
		<managed-bean-class>managedBeans.LivroMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>autorMBean</managed-bean-name>
		<managed-bean-class>managedBeans.AutorMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>estanteMBean</managed-bean-name>
		<managed-bean-class>managedBeans.EstanteMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>idiomaMBean</managed-bean-name>
		<managed-bean-class>managedBeans.IdiomaMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>usuarioMBean</managed-bean-name>
		<managed-bean-class>managedBeans.UsuarioMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<!-- *** NAVIGATION RULES *** -->
	<navigation-rule>
		<navigation-case>
			<from-action>#{usuarioMBean.fazerLogin}</from-action>
			<from-outcome>admin</from-outcome>
			<to-view-id>/pages_admin/index.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{usuarioMBean.fazerLogin}</from-action>
			<from-outcome>normal</from-outcome>
			<to-view-id>/pages_usuario/index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
		<navigation-rule>
		<navigation-case>
			<from-action>#{usuarioMBean.fazerLogout}</from-action>
			<to-view-id>/pages_usuario/index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages_admin/cadastro_categoria.jsf</from-view-id>
		<navigation-case>
			<from-outcome>true</from-outcome>
			<to-view-id>/pages_admin/index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_usuario/index.jsf</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/pages_admin/index.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>normal</from-outcome>
			<to-view-id>/pages_usuario/index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>cadastroCategoria</from-outcome>
			<to-view-id>/pages_admin/cadastro_categoria.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>listarCategorias</from-outcome>
			<to-view-id>/pages_admin/categoria.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>cadastroAutor</from-outcome>
			<to-view-id>/pages_admin/cadastro_autor.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>listarAutores</from-outcome>
			<to-view-id>/pages_admin/autor.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>cadastroEditora</from-outcome>
			<to-view-id>/pages_admin/cadastro_editora.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>listarEditoras</from-outcome>
			<to-view-id>/pages_admin/editora.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>cadastroEstante</from-outcome>
			<to-view-id>/pages_admin/cadastro_estante.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>listarEstantes</from-outcome>
			<to-view-id>/pages_admin/estante.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>cadastroLivro</from-outcome>
			<to-view-id>/pages_admin/cadastro_livro.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>listarLivros</from-outcome>
			<to-view-id>/pages_admin/livro.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cadastroUsuario</from-outcome>
			<to-view-id>/pages_admin/cadastro_usuario.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>listarUsuarios</from-outcome>
			<to-view-id>/pages_admin/usuario.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_admin/*</from-view-id>
		<navigation-case>
			<from-outcome>listarIdiomas</from-outcome>
			<to-view-id>/pages_admin/idioma.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages_usuario/*</from-view-id>
		<navigation-case>
			<from-outcome>listarUsuarios</from-outcome>
			<to-view-id>/pages_usuario/index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- *** APPLICATION CONFIG *** -->
	<application>
		<view-handler>org.ajax4jsf.application.AjaxViewHandler</view-handler>
		<!-- <view-handler>com.sun.facelets.FaceletViewHandler</view-handler> -->
	</application>
</faces-config>
